# -----------------------------------------
# Backend
# -----------------------------------------

APP_NAME=DongneFit Backend
BACKEND_URL=http://localhost:8000
API_V1_PREFIX=/api/v1
DEBUG=false

# Session
SECRET_KEY=your-secret-key-change-in-production
SESSION_EXPIRE_DAYS=7

# -----------------------------------------
# Frontend
# -----------------------------------------

# App Url
FRONTEND_CLIENT_APP_URL=http://localhost:5173
FRONTEND_ADMIN_APP_URL=http://localhost:5174

# -----------------------------------------
# Database
# -----------------------------------------

DATABASE_URL=postgresql+asyncpg://postgres:password@localhost:5432/realestate
DATABASE_ECHO=false

# -----------------------------------------
# 공공데이터
# -----------------------------------------

# 공공데이터포털 API 키 (https://www.data.go.kr/)
DATA_GO_KR_API_ENCODE_KEY=your_api_key_here
DATA_GO_KR_API_DECODE_KEY=your_api_key_here

# VWORLD API 키 (https://www.vworld.kr/)
VWORLD_API_KEY=your_api_key_here

# KOSIS Open API (https://KOSIS.kostat.go.kr/)
KOSIS_API_KEY=your_kosis_api_key_here

# 한국부동산원 R-ONE 부동산통계정보 API (https://www.reb.or.kr/r-one/portal/openapi/openApiIntroPage.do)
R_ONE_API_KEY=your_api_key_here

# 데이터 저장 경로 (기본값: ./data)
DATA_DIR=./pipeline/data

# 건축HUB

# -----------------------------------------
# THIRD PARTY
# -----------------------------------------

# OAuth - Google
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_CALLBACK_URL=http://localhost:8000/api/auth/google/callback

# OAuth - Kakao
KAKAO_CLIENT_ID=your-kakao-client-id
KAKAO_CLIENT_SECRET=your-kakao-client-secret
KAKAO_CALLBACK_URL=http://localhost:8000/api/auth/kakao/callback

# OpenAI (for content generation)
OPENAI_API_KEY=your-openai-api-key

# Anthropic (optional)
ANTHROPIC_API_KEY=your-anthropic-api-key

# Naver API
NAVER_MAP_PROVIDER=naver
NAVER_CLIENT_ID=your-client-id
NAVER_CLIENT_SECRET=your-client-secret

# -----------------------------------------
# 인프라
# -----------------------------------------

# AWS S3
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=ap-northeast-2
S3_BUCKET=your-s3-bucket-name

# -----------------------------------------
# 크롤링, 블로그, 자동화
# -----------------------------------------

# Selenium
SELENIUM_HEADLESS=true
SELENIUM_TIMEOUT=30

# Tistory Credentials
TISTORY_ID=your_tistory_id_here
TISTORY_PASSWORD=your_tistory_password_here
